<?xml version="1.0" encoding="utf-8" ?>
<v:BaseView 
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:v="clr-namespace:Contoso.Suggestions.UI.Views"
    xmlns:vm="clr-namespace:Contoso.Suggestions.Core.ViewModels;assembly=Contoso.Suggestions.Core"
    Title="{Binding Title}"
    x:Class="Contoso.Suggestions.UI.Views.MapPage"
    x:TypeArguments="vm:MapViewModel"
    x:DataType="vm:MapViewModel">

    <StackLayout>
        <Label Text="{Binding Title}" />
        <Button Text="About" Command="{Binding Navigation.NavigateAboutCommand}" />
        <Button Text="Go Home" Command="{Binding Navigation.NavigateHomeCommand}" />
    </StackLayout>

</v:BaseView>